{
  "name": "ngx-dropzone-pkg",
  "version": "20.0.0",
  "scripts": {
    "ng": "ng",
    "start:app": "ng serve app",
    "start:lib": "ng serve --watch --project=lib",
    "build:app": "ng build app",
    "build:lib": "ng build lib --configuration production",
    "test:app": "ng test app",
    "lint:lib": "ng lint lib",
    "e2e": "ng e2e",
    "npm:deploy": "npm run build:lib && cp README.md dist/ngx-dropzone/ && cd dist/ngx-dropzone && npm publish",
    "release": "cd projects/ngx-dropzone && npm version minor && cd ../.. && ng build --prod ngx-dropzone && cp README.md dist/ngx-dropzone/ && cd dist/ngx-dropzone && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-browser-dynamic": "^20.1.0",
    "@angular/router": "^20.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.6.3",
    "tslib": "^2.6.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/language-service": "^20.1.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^20.1.0",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "5.8.3"
  }
}